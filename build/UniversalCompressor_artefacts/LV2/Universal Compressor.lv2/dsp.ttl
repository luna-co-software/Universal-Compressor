plug:mode
	a lv2:Parameter ;
	rdfs:label "Mode" ;
	rdfs:range atom:Float ;
	lv2:default 2 ;
	lv2:minimum 0 ;
	lv2:maximum 3	 ;
	lv2:portProperty lv2:enumeration ;
	lv2:scalePoint [
		rdfs:label "Opto" ;
		rdf:value 0 ;
	], [
		rdfs:label "FET" ;
		rdf:value 1 ;
	], [
		rdfs:label "VCA" ;
		rdf:value 2 ;
	], [
		rdfs:label "Bus" ;
		rdf:value 3 ;
	] .

plug:bypass
	a lv2:Parameter ;
	rdfs:label "Bypass" ;
	rdfs:range atom:Float ;
	lv2:default 0 ;
	lv2:minimum 0 ;
	lv2:maximum 1	 ;
	lv2:portProperty lv2:enumeration , lv2:toggled ;
	lv2:scalePoint [
		rdfs:label "Off" ;
		rdf:value 0 ;
	], [
		rdfs:label "On" ;
		rdf:value 1 ;
	] .

plug:stereo_link
	a lv2:Parameter ;
	rdfs:label "Stereo Link" ;
	rdfs:range atom:Float ;
	lv2:default 100 ;
	lv2:minimum 0 ;
	lv2:maximum 100 .

plug:mix
	a lv2:Parameter ;
